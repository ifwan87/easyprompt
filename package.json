{
  "name": "easyprompt",
  "version": "1.0.0-beta.1",
  "description": "AI prompt optimization platform with multi-provider support",
  "private": true,
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "rm -rf .next node_modules",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "db:start": "./scripts/dev-db.sh start",
    "db:start:tools": "./scripts/dev-db.sh start --tools",
    "db:stop": "./scripts/dev-db.sh stop",
    "db:restart": "./scripts/dev-db.sh restart",
    "db:status": "./scripts/dev-db.sh status",
    "db:logs": "./scripts/dev-db.sh logs",
    "db:clean": "./scripts/dev-db.sh clean",
    "db:reset": "./scripts/dev-db.sh reset-db",
    "db:psql": "./scripts/dev-db.sh psql",
    "db:backup": "./scripts/dev-db.sh backup",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "setup:dev": "npm run db:start && npm run prisma:generate && npm run prisma:migrate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@google/generative-ai": "^0.21.0",
    "@huggingface/inference": "^2.8.1",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-tabs": "^1.1.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bcrypt": "^6.0.0",
    "@upstash/ratelimit": "^2.0.4",
    "@upstash/redis": "^1.34.3",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "ollama": "^0.6.3",
    "openai": "^4.73.0",
    "prisma": "^5.22.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "replicate": "^0.34.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.17",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.10",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.15.0",
    "eslint-config-next": "^16.0.3",
    "happy-dom": "^20.0.10",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript": "^5.7.2",
    "vitest": "^4.0.10"
  }
}
